<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Business_Type__c</fullName>
    <description>Updates field to Renewal Business if at least one Opp exists on the same Acct.
Tracks marketing impact, aiming for more new clients vs. return clients.
Helps measure client retention &amp; New vs. Return %.</description>
    <externalId>false</externalId>
    <label>New v. Return</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Renewal Business</fullName>
                <default>true</default>
                <label>Return Client</label>
            </value>
            <value>
                <fullName>New Business</fullName>
                <default>false</default>
                <label>New Client</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
